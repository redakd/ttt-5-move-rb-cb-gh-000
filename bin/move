#!/usr/bin/env ruby

require_relative '../lib/move.rb'

puts "Welcome to Tic Tac Toe!"
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
display_board(board)
puts "Where would you like to go?"

position = gets
position1 = position.to_i

index = input_to_index(position)

move(board,index)

display_board(board)
